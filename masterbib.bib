@Article{Muellner2015,
  author          = {Müllner, Fiona E. and Wierenga, Corette J. and Bonhoeffer, Tobias},
  title           = {Precision of Inhibition: Dendritic Inhibition by Individual GABAergic Synapses on Hippocampal Pyramidal Cells Is Confined in Space and Time.},

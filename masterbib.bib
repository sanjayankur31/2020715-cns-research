@Article{Doron2017,
  author          = {Doron, Michael and Chindemi, Giuseppe and Muller, Eilif and Markram, Henry and Segev, Idan},
  title           = {Timed Synaptic Inhibition Shapes NMDA Spikes, Influencing Local Dendritic Processing and Global I/O Properties of Cortical Neurons.},
